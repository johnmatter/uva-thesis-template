@article{Ryckebusch_2003,
    title = {Relativistic formulation of Glauber theory for A(e,e'p) reactions},
    journal = {Nuclear Physics A},
    volume = {728},
    number = {1},
    pages = {226-250},
    year = {2003},
    issn = {0375-9474},
